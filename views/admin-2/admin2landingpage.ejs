<script>
  var doc = new jsPDF()

  function generatePdf() {
    fetch('/admin/orderDataAdmin', {
        method: 'get',
        headers: {
          'Content-Type': 'application/json',
        }
      }).then(res => res.json())
      .then((res) => {
        // monthlyReport(res.dataMonth)
        console.log("1", res.dataMonth);
        document.getElementById('2').innerText = res.dataMonth.total
        document.getElementById('3').innerText = res.dataMonth.orders
        document.getElementById('4').innerText = res.dataMonth.totalOrder


      })
  }

  function generatePdfYearly() {
    fetch('/admin/orderDataAdminYearly', {
        method: 'get',
        headers: {
          'Content-Type': 'application/json',
        }
      }).then(res => res.json())
      .then((res) => {
        // monthlyReport(res.dataYearly)
        console.log("2", res.dataYearly);
        document.getElementById('6').innerText = res.dataYearly.total
        document.getElementById('7').innerText = res.dataYearly.orders
        document.getElementById('8').innerText = res.dataYearly.totalOrder

      })
  }

  function generatePdfDaily() {
    fetch('/admin/orderDataAdminDaily', {
        method: 'get',
        headers: {
          'Content-Type': 'application/json',
        }
      }).then(res => res.json())
      .then((res) => {
        // monthlyReport(res.dataTodayRevenue)
        console.log("3", res.dateTodayRevenue);
        document.getElementById('10').innerText = res.dateTodayRevenue.total
        document.getElementById('11').innerText = res.dateTodayRevenue.orders
        document.getElementById('12').innerText = res.dateTodayRevenue.totalOrder
        setTimeout(() => {
          generate()
        }, 500)

      })
  }


  function generate() {

    var a = document.getElementById('2').innerText
    var b = document.getElementById('3').innerText
    var c = document.getElementById('4').innerText
    var d = document.getElementById('6').innerText
    var e = document.getElementById('7').innerText
    var f = document.getElementById('8').innerText
    var g = document.getElementById('10').innerText
    var h = document.getElementById('11').innerText
    var i = document.getElementById('12').innerText

    var columns = [{
        title: "ID",
        dataKey: "id"
      },
      {
        title: "Total",
        dataKey: "Total"
      },
      {
        title: "Orders",
        dataKey: "Orders"
      },
      {
        title: "Total Orders",
        dataKey: "Total Order"
      }
    ];
    var rows = [{
        "id": 1,
        "Total": "Rs " + a,
        "Orders": b,
        "Total Order": c
      },


    ];
    var rows1 = [{
      "id": 2,
      "Total": "Rs " + d,
      "Orders": e,
      "Total Order": f
    }, ]

    var rows2 = [{
      "id": 3,
      "Total": "Rs " + g,
      "Orders": h,
      "Total Order": i
    }, ]

    var doc = new jsPDF('p', 'pt');
    var doc1 = new jsPDF('p', 'pt');
    var doc2 = new jsPDF('p', 'pt');

    var header = function(data) {
      doc.setFontSize(18);
      doc.setTextColor(40);
      doc.setFontStyle('normal');
      doc.text("Monthly Report", data.settings.margin.left, 50);
    };
    doc.autoTable(columns, rows, {
      margin: {
        top: 80
      },
      beforePageContent: header
    });


    var header1 = function(data) {
      doc1.setFontSize(18);
      doc1.setTextColor(40);
      doc1.setFontStyle('normal');
      doc1.text("Yearly Report", data.settings.margin.left, 50);
    };
    doc1.autoTable(columns, rows1, {
      margin: {
        top: 80
      },
      beforePageContent: header1
    });


    var header2 = function(data) {
      doc2.setFontSize(18);
      doc2.setTextColor(40);
      doc2.setFontStyle('normal');
      doc2.text("Daily Report", data.settings.margin.left, 50);
    };
    doc2.autoTable(columns, rows2, {
      margin: {
        top: 80
      },
      beforePageContent: header2
    });

    doc.save("Monthly-Sales.pdf");
    doc1.save("Yearly-Sales.pdf")
    doc2.save("Daily-Sales.pdf")
  }

  function generateExcel() {
    $.ajax({
      url: '/admin/generateExcel',
      method: 'get',
      success: (res) => {
        location.reload()
      }
    })
  }
</script>